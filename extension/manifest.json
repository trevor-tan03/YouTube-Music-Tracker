{
	"name": "YouTube Listening Tracker",
	"description": "Extension to track listening data from YouTube",
	"version": "1.0",
	"manifest_version": 3,
	"action": {
		"default_popup": "popup.html",
		"default_icon": {
			"16": "/images/icon16.png",
			"32": "/images/icon32.png",
			"48": "/images/icon48.png",
			"128": "/images/icon128.png"
		}
	},
	"content_scripts": [
		{
			"matches": ["https://www.youtube.com/*"],
			"js": ["scripts/main.js"]
		}
	],
	"permissions": []
}
